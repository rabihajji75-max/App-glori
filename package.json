{
  "name": "freefire-glory-pro",
  "version": "1.0.0",
  "description": "تطبيق ويب متكامل 100% لإدارة وجمع القلوري في Free Fire",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "client": "live-server public --port=3001",
    "dev:all": "concurrently \"npm run dev\" \"npm run client\"",
    "build": "echo 'No build required for PWA'",
    "deploy": "vercel --prod",
    "deploy:server": "vercel server --prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/ server/",
    "format": "prettier --write \"**/*.{js,json,html,css}\"",
    "db:migrate": "node server/database/migrate.js",
    "db:seed": "node server/database/seed.js",
    "security:check": "npm audit",
    "generate-icons": "node scripts/generate-icons.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.0.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "validator": "^13.9.0",
    "rate-limit-redis": "^3.0.1",
    "express-rate-limit": "^6.8.0",
    "sequelize": "^6.32.0",
    "sqlite3": "^5.1.6",
    "axios": "^1.4.0",
    "node-cron": "^3.0.2",
    "winston": "^3.9.0",
    "socket.io": "^4.6.1",
    "node-fetch": "^3.3.1",
    "crypto-js": "^4.1.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "concurrently": "^8.2.0",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "eslint": "^8.42.0",
    "prettier": "^3.0.0",
    "live-server": "^1.2.2",
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead"
  ],
  "keywords": [
    "freefire",
    "glory",
    "pwa",
    "web-app",
    "gaming",
    "automation"
  ],
  "author": "DENJITX",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rabihajjj75-max/freefire-glory-pro"
  },
  "bugs": {
    "url": "https://github.com/rabihajjj75-max/freefire-glory-pro/issues"
  },
  "homepage": "https://freefire-glory-pro.vercel.app"
}
